<!DOCTYPE html>
<html>
  <head>
    <title>RailBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav>
      <ul>
        <li><a href="/">Home</a>
        <li><%= link_to 'New', new_post_path(@post) %> </li>
      </ul>
    </nav>
    <%= yield %>
  </body>
</html>
